{
  "name": "a-defection",
  "version": "1.0.0",
  "description": "An interactive narrative platform connecting stories to real-world events",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "test": "jest",
    "client": "cd client && npm start",
    "client-install": "cd client && npm install",
    "server-install": "npm install",
    "install-all": "npm run server-install && npm run client-install",
    "dev-full": "concurrently \"npm run dev\" \"npm run client\"",
    "build": "cd client && npm run build",
    "deploy": "npm run build && npm start"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.5",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/a-defection.git"
  },
  "keywords": [
    "interactive",
    "narrative",
    "prediction",
    "blockchain",
    "ai"
  ],
  "author": "Your Name",
  "license": "MIT"
} 